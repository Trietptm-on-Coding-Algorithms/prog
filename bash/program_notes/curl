


verbose
	-v

hide progress
	-s

extra info
	-w "format"

write output
	-o "file"

timeout
	-m seconds

POST
	-d "STrING"

multipart formpost
	-F name=@file

PUT
	-T file

HEAD 
	-I

Custom Method
	-X "Method"

basic auth
	-u user:password

read cookiejar
	-b "file"

wrote cookiejar
	-c "file"

send cookies
	-b "c=1; d=2"

user-agent
	-A "string"

Use proxy
	-x host:port

Headers 
	-H "name: value"

follow redirects
	-L

gzipped response
	--compressed

insecure HTTPs
	-k
